{
    "name": "minimal-artistic-portfolio",
    "version": "1.0.0",
    "description": "![Wordpress](https://img.shields.io/wordpress/v/akismet.svg) ![Status](https://img.shields.io/pypi/status/Django.svg)",
    "main": "src/index.js",
    "scripts": {
        "build:assets": "webpack --mode production --config webpack.config.js",
        "build:archive": "dir-archiver --src . --dest ../minimal-artistic-theme.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore .vscode README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map .babelrc .jscsrc .jshintignore .prettierrc postcss.config.js svgo.config.js webpack.config.js src",
        "build": "npm run build:assets && npm run build:archive",
        "watch": "webpack serve --mode development --config webpack.config.js",
        "compile:rtl": "rtlcss style.css style-rtl.css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nclslbrn/Minimal-artist-portfolio.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nclslbrn/Minimal-artist-portfolio/issues"
    },
    "homepage": "https://github.com/nclslbrn/Minimal-artist-portfolio#readme",
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@wordpress/scripts": "^17.1.0",
        "autoprefixer": "^10.3.1",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "cssnano": "^5.0.7",
        "dir-archiver": "^1.2.0",
        "expose-loader": "^3.0.0",
        "fluidbox": "^2.0.5",
        "image-minimizer-webpack-plugin": "^2.2.0",
        "image-webpack-loader": "^7.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^9.0.0",
        "plyr": "^3.6.8",
        "postcss": "^8.3.6",
        "postcss-loader": "^6.1.1",
        "postcss-preset-env": "^6.7.0",
        "rtlcss": "^3.3.0",
        "style-loader": "^3.2.1",
        "webpack-dev-middleware": "^5.0.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-hot-middleware": "^2.25.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.11.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.2",
        "babel-preset": "^1.1.7",
        "browser-sync": "^2.27.5",
        "browser-sync-webpack-plugin": "^2.3.0",
        "copy-webpack-plugin": "^9.0.1",
        "core-js": "^3.16.0",
        "css-loader": "^6.2.0",
        "file-loader": "^6.2.0",
        "imagemin": "^8.0.0",
        "jquery": "^3.6.0",
        "mini-css-extract-plugin": "^2.2.0",
        "plumber-sass": "^0.7.7",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.37.5",
        "sass-loader": "^12.1.0",
        "webpack": "^5.49.0",
        "webpack-cli": "^4.7.2",
        "webpackbar": "^5.0.0-3"
    },
    "browserslist": "last 2 versions",
    "rtlcssConfig": {
        "options": {
            "autoRename": false,
            "autoRenameStrict": false,
            "blacklist": {},
            "clean": true,
            "greedy": false,
            "processUrls": false,
            "stringMap": []
        },
        "plugins": [],
        "map": false
    }
